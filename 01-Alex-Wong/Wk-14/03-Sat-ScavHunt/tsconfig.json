{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "typeRoots": ["./node_modules/@types"],
        "types": [
            "node",
            "express"
        ],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "node16",
        "noEmit": false,
        "emitDeclarationOnly": true,
        "declaration": true,
        "allowImportingTsExtensions": true,
    },
    "include": [
        "./**/*.ts",
        "main.ts"
    ]
}
  